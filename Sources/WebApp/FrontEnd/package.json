{
  "name": "workshop",
  "version": "1.0.0",
  "description": "This is a demoapp",
  "main": "index.js",
  "scripts": {
    "build": "rimraf wwwroot/* && webpack --production",
    "debug": "rimraf wwwroot/* && webpack --development",
    "start": "concurrently \"npm:start-microservice-auth\" \"npm:start-microservice-mdat\" \"npm:start-webfrontend\" ",
    "start-microservice-auth": "dotnet run -p ../JoergIsAGeek.Workshop.Enterprise.ServiceLayer.Authentication/JoergIsAGeek.Workshop.Enterprise.ServiceLayer.csproj",
    "start-microservice-mdat": "dotnet run -p ../JoergIsAGeek.Workshop.Enterprise.ServiceLayer.MachineData/JoergIsAGeek.Workshop.Enterprise.ServiceLayer.csproj",
    "start-webfrontend": "dotnet run -p ./JoergIsAGeek.Workshop.Enterprise.WebApplication/JoergIsAGeek.Workshop.Enterprise.WebApplication.csproj",
    "proxy": "rimraf \"Connected Services\" && concurrently \"npm run proxy-auth\" \"npm run proxy-data\"",
    "proxy-auth": "nswag openapi2csclient /input:Setup/Authentication-proxy-3.json /output:\"Connected Services/AuthenticationService/ServiceProxy.cs\" /namespace:JoergIsAGeek.ServiceProxy.Authentication",
    "proxy-data": "nswag openapi2csclient /input:Setup/MachineData-proxy-3.json /output:\"Connected Services/MachineDataService/ServiceProxy.cs\" /namespace:JoergIsAGeek.ServiceProxy.MachineData"
  },
  "repository": "https://github.com/joergkrause/mvccore2ng-boilerplate",
  "author": "Joerg Krause - www.joergkrause.de",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@ng-bootstrap/ng-bootstrap": "^6.0.2",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.4.0",
    "core-js": "^3.4.3",
    "font-awesome": "^4.7.0",
    "ng2-charts": "^2.3.0",
    "rxjs": "^6.5.0",
    "rxjs-compat": "^6.5.0",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.0"
  },
  "devDependencies": {
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@ngtools/webpack": "^9.1.0",
    "@types/node": "^10.0.0",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "del": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.9.3",
    "nswag": "^13.1.6",
    "open": "^7.0.0",
    "raw-loader": "^4.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "typescript": "^3.6.4",
    "webpack": "^4.40.0",
    "webpack-cli": "^3.3.10",
    "rimraf": "3.0.2"
  }
}
