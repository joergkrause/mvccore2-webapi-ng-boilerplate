  "description": "This is a demoapp",
  "main": "index.js",
  "scripts": {
    "build": "rimraf wwwroot/* && webpack --production",
    "build-no-proxy": "rimraf wwwroot/* && webpack --production",
    "debug": "rimraf wwwroot/* && webpack --development",
    "proxy-ps": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./CreateSwaggerAPIProxy.ps1",
    "proxy": "node ./Setup/setup bp",
    "start": "concurrently \"npm:start-microservice-auth\" \"npm:start-microservice-mdat\" \"npm:start-webfrontend\" ",
    "start-microservice-auth": "dotnet run -p ../JoergIsAGeek.Workshop.Enterprise.ServiceLayer.Authentication/JoergIsAGeek.Workshop.Enterprise.ServiceLayer.csproj",
    "start-microservice-mdat": "dotnet run -p ../JoergIsAGeek.Workshop.Enterprise.ServiceLayer.MachineData/JoergIsAGeek.Workshop.Enterprise.ServiceLayer.csproj",
    "start-webfrontend": "dotnet run -p ./JoergIsAGeek.Workshop.Enterprise.WebApplication/JoergIsAGeek.Workshop.Enterprise.WebApplication.csproj",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
